{
  "name": "fire-tracker",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/fire-tracker/src",
  "prefix": "tt",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/fire-tracker",
        "index": "apps/fire-tracker/src/index.html",
        "main": "apps/fire-tracker/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/fire-tracker/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": ["apps/fire-tracker/src/favicon.ico", "apps/fire-tracker/src/assets"],
        "styles": [
          "@angular/material/prebuilt-themes/indigo-pink.css",
          "apps/fire-tracker/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "fire-tracker:build:production"
        },
        "development": {
          "browserTarget": "fire-tracker:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "fire-tracker:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/fire-tracker/**/*.ts", "apps/fire-tracker/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/fire-tracker/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "deploy": {
      "executor": "@angular/fire:deploy",
      "options": {
        "prerender": false,
        "ssr": false,
        "browserTarget": "fire-tracker:build:production",
        "firebaseProject": "fire-tracker-b48f3",
        "firebaseHostingSite": "fire-tracker-b48f3"
      }
    }
  },
  "tags": []
}
