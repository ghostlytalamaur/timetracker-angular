{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "../../dist/frontend/out-tsc",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@app/store": [
        "src/app/store/index.ts"
      ],
      "@app/core/*": [
        "src/app/core/*"
      ],
      "@app/shared": [
        "src/app/shared/index.ts"
      ],
      "@app/shared/*": [
        "src/app/shared/*"
      ],
      "@app/storage/models": [
        "../functions/src/interfaces/index.ts"
      ]
    }
  }
}
