{
  "name": "core-api",
  "projectType": "library",
  "sourceRoot": "libs/core/api/src",
  "prefix": "tt",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/core/api/src/**/*.ts",
          "libs/core/api/src/**/*.html"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/core/api"
      ],
      "options": {
        "jestConfig": "libs/core/api/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
